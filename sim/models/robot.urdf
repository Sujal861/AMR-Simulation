<?xml version="1.0"?>
<robot name="autonomous_mobile_manipulator">
  <!-- Main Base Platform -->
  <link name="base_platform">
    <visual>
      <geometry>
        <box size="1.2 0.8 0.15"/>
      </geometry>
      <material name="base_material">
        <color rgba="0.3 0.3 0.3 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="1.2 0.8 0.15"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="80.0"/>
      <inertia ixx="8.0" ixy="0.0" ixz="0.0" iyy="6.0" iyz="0.0" izz="4.0"/>
    </inertial>
  </link>

  <!-- Enhanced Mecanum Wheels with Better Design -->
  <!-- Front Left Wheel -->
  <link name="wheel_fl">
    <visual>
      <geometry>
        <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <material name="wheel_material">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.15" length="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="3.0"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>

  <joint name="wheel_fl_joint" type="continuous">
    <parent link="base_platform"/>
    <child link="wheel_fl"/>
    <origin xyz="0.45 0.35 0.075" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-20" upper="20" effort="100" velocity="10.0"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

  <!-- Front Right Wheel -->
  <link name="wheel_fr">
    <visual>
      <geometry>
        <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <material name="wheel_material"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.15" length="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="3.0"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>

  <joint name="wheel_fr_joint" type="continuous">
    <parent link="base_platform"/>
    <child link="wheel_fr"/>
    <origin xyz="0.45 -0.35 0.075" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-20" upper="20" effort="100" velocity="10.0"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

  <!-- Rear Left Wheel -->
  <link name="wheel_rl">
    <visual>
      <geometry>
        <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <material name="wheel_material"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.15" length="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="3.0"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>

  <joint name="wheel_rl_joint" type="continuous">
    <parent link="base_platform"/>
    <child link="wheel_rl"/>
    <origin xyz="-0.45 0.35 0.075" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-20" upper="20" effort="100" velocity="10.0"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

  <!-- Rear Right Wheel -->
  <link name="wheel_rr">
    <visual>
      <geometry>
        <cylinder radius="0.15" length="0.1"/>
      </geometry>
      <material name="wheel_material"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.15" length="0.1"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="3.0"/>
      <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03"/>
    </inertial>
  </link>

  <joint name="wheel_rr_joint" type="continuous">
    <parent link="base_platform"/>
    <child link="wheel_rr"/>
    <origin xyz="-0.45 -0.35 0.075" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-20" upper="20" effort="100" velocity="10.0"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

  <!-- Enhanced Sensor Tower -->
  <link name="sensor_tower">
    <visual>
      <geometry>
        <cylinder radius="0.08" length="0.4"/>
      </geometry>
      <material name="sensor_material">
        <color rgba="0.2 0.2 0.2 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.08" length="0.4"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="3.0"/>
      <inertia ixx="0.02" ixy="0.0" ixz="0.0" iyy="0.02" iyz="0.0" izz="0.02"/>
    </inertial>
  </link>

  <joint name="sensor_tower_joint" type="fixed">
    <parent link="base_platform"/>
    <child link="sensor_tower"/>
    <origin xyz="0 0 0.275" rpy="0 0 0"/>
  </joint>

  <!-- LiDAR Sensor -->
  <link name="lidar_sensor">
    <visual>
      <geometry>
        <cylinder radius="0.06" length="0.04"/>
      </geometry>
      <material name="lidar_material">
        <color rgba="0.8 0.2 0.2 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.06" length="0.04"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="lidar_joint" type="fixed">
    <parent link="sensor_tower"/>
    <child link="lidar_sensor"/>
    <origin xyz="0 0 0.22" rpy="0 0 0"/>
  </joint>

  <!-- Camera Mount -->
  <link name="camera_mount">
    <visual>
      <geometry>
        <box size="0.1 0.1 0.05"/>
      </geometry>
      <material name="camera_material">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.1 0.1 0.05"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.3"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>
  </link>

  <joint name="camera_mount_joint" type="fixed">
    <parent link="sensor_tower"/>
    <child link="camera_mount"/>
    <origin xyz="0.15 0 0.15" rpy="0 0 0"/>
  </joint>

  <!-- Enhanced Manipulator Base -->
  <link name="manipulator_base">
    <visual>
      <geometry>
        <cylinder radius="0.15" length="0.25"/>
      </geometry>
      <material name="manipulator_material">
        <color rgba="0.4 0.4 0.4 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.15" length="0.25"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="8.0"/>
      <inertia ixx="0.08" ixy="0.0" ixz="0.0" iyy="0.08" iyz="0.0" izz="0.08"/>
    </inertial>
  </link>

  <joint name="manipulator_base_joint" type="fixed">
    <parent link="base_platform"/>
    <child link="manipulator_base"/>
    <origin xyz="0.3 0 0.175" rpy="0 0 0"/>
  </joint>

  <!-- Enhanced 6-DOF Manipulator Arm for Pick-and-Place -->
  <!-- Shoulder Pan -->
  <link name="shoulder_pan">
    <visual>
      <geometry>
        <cylinder radius="0.1" length="0.2"/>
      </geometry>
      <material name="arm_material">
        <color rgba="0.6 0.6 0.6 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.1" length="0.2"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="4.0"/>
      <inertia ixx="0.04" ixy="0.0" ixz="0.0" iyy="0.04" iyz="0.0" izz="0.04"/>
    </inertial>
  </link>

  <joint name="shoulder_pan_joint" type="revolute">
    <parent link="manipulator_base"/>
    <child link="shoulder_pan"/>
    <origin xyz="0 0 0.125" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14" upper="3.14" effort="200" velocity="3.0"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

  <!-- Shoulder Lift -->
  <link name="shoulder_lift">
    <visual>
      <geometry>
        <cylinder radius="0.08" length="0.4"/>
      </geometry>
      <material name="arm_material"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.08" length="0.4"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="3.5"/>
      <inertia ixx="0.035" ixy="0.0" ixz="0.0" iyy="0.035" iyz="0.0" izz="0.035"/>
    </inertial>
  </link>

  <joint name="shoulder_lift_joint" type="revolute">
    <parent link="shoulder_pan"/>
    <child link="shoulder_lift"/>
    <origin xyz="0 0 0.1" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.0" upper="2.0" effort="200" velocity="3.0"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

  <!-- Upper Arm -->
  <link name="upper_arm">
    <visual>
      <geometry>
        <cylinder radius="0.07" length="0.45"/>
      </geometry>
      <material name="arm_material"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.07" length="0.45"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.5"/>
      <inertia ixx="0.025" ixy="0.0" ixz="0.0" iyy="0.025" iyz="0.0" izz="0.025"/>
    </inertial>
  </link>

  <joint name="upper_arm_joint" type="revolute">
    <parent link="shoulder_lift"/>
    <child link="upper_arm"/>
    <origin xyz="0 0 0.2" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="150" velocity="3.0"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

  <!-- Forearm -->
  <link name="forearm">
    <visual>
      <geometry>
        <cylinder radius="0.06" length="0.4"/>
      </geometry>
      <material name="arm_material"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.06" length="0.4"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.0"/>
      <inertia ixx="0.02" ixy="0.0" ixz="0.0" iyy="0.02" iyz="0.0" izz="0.02"/>
    </inertial>
  </link>

  <joint name="forearm_joint" type="revolute">
    <parent link="upper_arm"/>
    <child link="forearm"/>
    <origin xyz="0 0 0.225" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14" upper="3.14" effort="120" velocity="3.0"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

  <!-- Wrist Flex -->
  <link name="wrist_flex">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.18"/>
      </geometry>
      <material name="arm_material"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.18"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.2"/>
      <inertia ixx="0.006" ixy="0.0" ixz="0.0" iyy="0.006" iyz="0.0" izz="0.006"/>
    </inertial>
  </link>

  <joint name="wrist_flex_joint" type="revolute">
    <parent link="forearm"/>
    <child link="wrist_flex"/>
    <origin xyz="0 0 0.09" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="80" velocity="3.0"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

  <!-- Wrist Rotate -->
  <link name="wrist_rotate">
    <visual>
      <geometry>
        <cylinder radius="0.05" length="0.15"/>
      </geometry>
      <material name="arm_material"/>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.05" length="0.15"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.005" ixy="0.0" ixz="0.0" iyy="0.005" iyz="0.0" izz="0.005"/>
    </inertial>
  </link>

  <joint name="wrist_rotate_joint" type="revolute">
    <parent link="wrist_flex"/>
    <child link="wrist_rotate"/>
    <origin xyz="0 0 0.075" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14" upper="3.14" effort="60" velocity="3.0"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

  <!-- Enhanced End Effector with Better Gripper -->
  <link name="end_effector">
    <visual>
      <geometry>
        <cylinder radius="0.04" length="0.2"/>
      </geometry>
      <material name="gripper_material">
        <color rgba="0.8 0.8 0.8 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.04" length="0.2"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.8"/>
      <inertia ixx="0.002" ixy="0.0" ixz="0.0" iyy="0.002" iyz="0.0" izz="0.002"/>
    </inertial>
  </link>

  <joint name="end_effector_joint" type="fixed">
    <parent link="wrist_rotate"/>
    <child link="end_effector"/>
    <origin xyz="0 0 0.075" rpy="0 0 0"/>
  </joint>

  <!-- Enhanced Gripper Left Finger -->
  <link name="gripper_left">
    <visual>
      <geometry>
        <box size="0.025 0.025 0.12"/>
      </geometry>
      <material name="gripper_material"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.025 0.025 0.12"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.15"/>
      <inertia ixx="0.002" ixy="0.0" ixz="0.0" iyy="0.002" iyz="0.0" izz="0.002"/>
    </inertial>
  </link>

  <joint name="gripper_left_joint" type="prismatic">
    <parent link="end_effector"/>
    <child link="gripper_left"/>
    <origin xyz="0 0.025 0.06" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="0" upper="0.05" effort="50" velocity="1.0"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

  <!-- Enhanced Gripper Right Finger -->
  <link name="gripper_right">
    <visual>
      <geometry>
        <box size="0.025 0.025 0.12"/>
      </geometry>
      <material name="gripper_material"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.025 0.025 0.12"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.15"/>
      <inertia ixx="0.002" ixy="0.0" ixz="0.0" iyy="0.002" iyz="0.0" izz="0.002"/>
    </inertial>
  </link>

  <joint name="gripper_right_joint" type="prismatic">
    <parent link="end_effector"/>
    <child link="gripper_right"/>
    <origin xyz="0 -0.025 0.06" rpy="0 0 0"/>
    <axis xyz="0 -1 0"/>
    <limit lower="0" upper="0.05" effort="50" velocity="1.0"/>
    <dynamics damping="0.1" friction="0.1"/>
  </joint>

</robot>
